{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@lib/*": ["lib/*"],
      "@models/*": ["models/*"],
      // "@config/*": ["config/*"],
      "@components/*": ["components/*"],
      // "@utils/*": ["utils/*"],
      // "@primitives/*": ["primitives/*"],
      // "@pages/*": ["pages/*"],
      // "@graphql/*": ["graphql/*"],
      "@svg": ["svg"]
    }
  },
  "include": ["**/*.js", "*.js"],
  "exclude": ["node_modules"]
}
